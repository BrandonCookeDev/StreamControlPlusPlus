{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"indent": [
			true,
			"tabs",
			4
		],
		"quotemark": [
			true,
			"single"
		],
		"semicolon": [
			true,
			"never"
		],
		"curly": false,
		"eofline": false,
		"one-line": false,
		"only-arrow-functions": true,
		"whitespace": false,
		"no-trailing-whitespace": false,
		"ordered-imports": false,
		"no-namespace": false,
		"triple-equals": false
	},
	"rulesDirectory": []
}